<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<VBox prefHeight="640.0" prefWidth="960.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.isachenkoff.project_statistics.MainController">
   <children>
      <ToolBar>
         <items>
            <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onNew" prefHeight="16.0" prefWidth="16.0" styleClass="tool-button">
               <graphic>
                  <Group>
                     <children>
                        <Line endY="10.0" layoutX="5.0" styleClass="line" />
                        <Line endX="10.0" layoutY="5.0" styleClass="line" />
                     </children>
                  </Group>
               </graphic>
            </Button>
         </items>
      </ToolBar>
      <TabPane fx:id="tabPane" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
