<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.isachenkoff.project_statistics.FileTypesTableController">
   <columns>
      <TableColumn fx:id="fileTypeColumn" prefWidth="110.0" text="Тип файла" />
      <TableColumn fx:id="filesCountColumn" prefWidth="110.0" style="-fx-alignment: CENTER_RIGHT;" text="Всего файлов" />
      <TableColumn fx:id="totalLinesColumn" prefWidth="105.0" style="-fx-alignment: CENTER_RIGHT;" text="Всего строк" />
      <TableColumn fx:id="notEmptyLinesColumn" prefWidth="120.0" style="-fx-alignment: CENTER_RIGHT;" text="Непустых строк" />
   </columns>
</TableView>
